{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "pretestflight": "yarn lint && yarn test",
    "testflight": "expo build:ios --type archive && expo upload:ios"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "setupFiles": [
      "<rootDir>/src/testUtils/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/testUtils",
      "<rootDir>/src/themes/nativeBase"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "0.1.5",
    "@react-navigation/bottom-tabs": "^5.2.7",
    "@react-navigation/drawer": "^5.5.0",
    "@react-navigation/native": "^5.0.9",
    "@react-navigation/stack": "^5.2.11",
    "@reduxjs/toolkit": "^1.4.0",
    "base-64": "^0.1.0",
    "decimal.js": "^10.2.0",
    "expo": "^37.0.7",
    "expo-constants": "^9.1.1",
    "firebase": "^10.9.0",
    "i18next": "^19.6.3",
    "intl": "^1.2.5",
    "lodash": "^4.17.15",
    "moment": "^2.27.0",
    "native-base": "2.13.8",
    "normalizr": "^3.6.0",
    "prop-types": "^15.7.2",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-hook-form": "^6.0.8",
    "react-i18next": "^11.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
    "react-native-easy-grid": "^0.2.2",
    "react-native-gesture-handler": "~1.5.0",
    "react-native-reanimated": "~1.4.0",
    "react-native-safe-area-context": "0.6.0",
    "react-native-screens": "2.0.0-alpha.12",
    "react-native-side-menu": "^1.1.3",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-web": "~0.11.7",
    "react-navigation": "^4.2.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "sentry-expo": "^2.1.2",
    "expo-font": "~8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~8.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "jest-expo": "^37.0.0",
    "prettier": "^2.0.4",
    "react-native-testing-library": "^1.14.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4"
  },
  "private": true
}
